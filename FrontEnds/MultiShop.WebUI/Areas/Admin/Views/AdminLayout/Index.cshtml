
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Component.InvokeAsync("_AdminLayoutHeadComponentPartial")

<body class="sidebar-mini">
    <div class="wrapper">

        <!-- Main Header -->
        @await Component.InvokeAsync("_AdminLayoutHeaderComponentPartial")
        <!-- Left side column. contains the logo and sidebar -->
        @await Component.InvokeAsync("_AdminLayoutSidebarComponentPartial")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>@ViewBag.v3</h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-home"></i> @ViewBag.v1</a></li>
                    <li class="active"><i class="fa fa-envelope-o"></i> @ViewBag.v2</li>
                    <li class="active"><i class="fa fa-bars"></i>@ViewBag.v3</li>
                </ol>
            </section>

            <!-- Main content -->
            @RenderBody()

          
            <!-- content -->
        </div>
        <!-- content-wrapper -->
        <!-- Main Footer -->
        @await Component.InvokeAsync("_AdminLayoutFooterComponentPartial")
    </div>
    <!-- wrapper -->
    <!-- jQuery -->
    @await Component.InvokeAsync("_AdminLayoutScriptComponentPartial")
</body>
</html>