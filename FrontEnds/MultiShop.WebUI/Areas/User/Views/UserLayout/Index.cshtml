
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_UserLayoutHeadComponentPartial")

<body>
    <div class="container-scroller d-flex">
        @await Component.InvokeAsync("_UserLayoutSidebarComponentPartial")

        <div class="container-fluid page-body-wrapper">
            <!-- partial:../../partials/_navbar.html -->
            @await Component.InvokeAsync("_UserLayoutNavbarComponentPartial")
            
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    <div class="row">
                     @RenderBody()
                    </div>
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:../../partials/_footer.html -->
                @await Component.InvokeAsync("_UserLayoutFooterComponentPartial")
            
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>

    @await Component.InvokeAsync("_UserLayoutScriptComponentPartial")

  
</body>

</html>
