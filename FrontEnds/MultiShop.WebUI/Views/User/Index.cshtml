@model UserDetailViewModel
@{
    Layout = null;
}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">



<div class="container">
    <br />
    <h3>Kullanıcı Profili Bilgisi</h3>
    <div class="alert alert-info">Kullanıcı Numarası:@Model.Id</div>
    <div class="alert alert-danger">Kullanıcı Adı:@Model.Name</div>
    <div class="alert alert-success">Kullanıcı Soyadı:@Model.Surame</div>
    <div class="alert alert-primary">Kullanıcı Emaili:@Model.Email</div>
    <div class="alert alert-secondary">Kullanıcının Kullanıcı Adı:@Model.UserName</div>
</div>






